{
    "name": "holgerk/assert-golden",
    "description": "Same as assertEquals, but if null is passed as expectation, null is automatically replaced with the actual value",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "phpunit",
        "xunit",
        "testing"
    ],
    "autoload": {
        "psr-4": {
            "Holgerk\\AssertGolden\\": "src/"
        },
        "files": [
            "src/Autoload.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Holgerk\\AssertGolden\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Holger Kohnen",
            "email": "h.kohnen@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "nikic/php-parser": "^5.0",
        "symfony/var-exporter": "^7.0",
        "phpunit/phpunit": "^9||^10||^11"
    }
}
